{
  "name": "@mite/nitrogen",
  "version": "0.0.1",
  "description": "A template for Nitrogen Modules.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "bootstrap": "bun --filter=\"**\" install && bun tsc && bun --filter=\"in-browser-app-example\" pods",
    "typescript": "bun --filter=\"**\" typescript",
    "clean": "rm -rf package/tsconfig.tsbuildinfo package/node_modules package/lib",
    "spec": "cd package && bun specs && cd .."
  },
  "workspaces": [
    "example",
    "package"
  ],
  "packageManager": "bun@1.1.34",
  "author": "Henry Paulino <me@henrypl.com> (https://github.com/corasan)",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "nitro-codegen": "^0.16.2"
  }
}
